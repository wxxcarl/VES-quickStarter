{
    "name": "io-visual-front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "dev": "npm run serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "core-js": "^2.6.5",
        "element-ui": "^2.9.1",
        "vue": "^2.6.10",
        "vue-echarts": "^4.0.3",
        "vue-router": "^3.0.3",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.0.0",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "husky": "^2.4.0",
        "lint-staged": "^8.1.5",
        "node-sass": "^4.9.0",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "plugins": [
            "vue"
        ],
        "rules": {
            "indent": [
                "warn",
                4
            ],
            "space-before-function-paren": [
                "error",
                "never"
            ],
            "no-console": 0,
            "no-debugger": 0,
            "quotes": [
                2,
                "single"
            ],
            "no-unused-vars": 1,
            "semi": [
                "error",
                "never"
            ],
            "no-undef": 2,
            "no-use-before-define": 2,
            "eqeqeq": 1,
            "comma-dangle": 0,
            "one-var": 0,
            "vue/html-indent": [
                "error",
                4
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
